<body id="top">

<div id="overlayer"></div>
<div class="loader">
  <div class="spinner-border text-primary" role="status">
    <span class="sr-only">Loading...</span>
  </div>
</div>


<div class="site-wrap">

  <div class="site-mobile-menu site-navbar-target">
    <div class="site-mobile-menu-header">
      <div class="site-mobile-menu-close mt-3">
        <span class="icon-close2 js-menu-toggle"></span>
      </div>
    </div>
    <div class="site-mobile-menu-body"></div>
  </div> <!-- .site-mobile-menu -->


  <!-- NAVBAR -->
  <%= render partial: 'shared/header' %>
  <!-- HOME -->
  <section class="section-hero overlay inner-page bg-image" style="background-image: url('/assets/hero_1.jpg');" id="home-section">
    <div class="container">
      <div class="row">
        <div class="col-md-7">
          <h1 class="text-white font-weight-bold">Frequently Ask Questions</h1>
          <div class="custom-breadcrumbs">
            <a href="index.html">Home</a> <span class="mx-2 slash">/</span>
            <span class="text-white"><strong>FAQ</strong></span>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="site-section" id="accordion">
    <div class="container">

      <div class="row accordion justify-content-center block__76208">
        <div class="col-lg-6">
          <img src="/assets/sq_img_8.jpg" alt="Image" class="img-fluid rounded">
        </div>
        <% if StaticPage.last.present? %>
          <%= StaticPage.last.faq.html_safe %>
        <% else %>
          <%= render partial: 'dashboards/faq' %>
        <% end %>

      </div>
    </div>
  </section>

  <section class="site-section bg-light">
    <div class="container">
      <div class="row mb-5">
        <div class="col-12 text-center" data-aos="fade">
          <h2 class="section-title mb-3">Happy Candidates Says</h2>
        </div>
      </div>
      <% if StaticPage.last.present? %>
        <%= StaticPage.last.reviews.html_safe %>
      <% else %>
        <%= render partial: 'dashboards/review' %>

      <% end %>
    </div>
  </section>
</div>
